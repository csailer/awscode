{
  "name": "awscode",
  "version": "1.0.0",
  "description": "AWS Desktop UI",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "clean": "rm -rf .fusebox out dist coverage",
    "compile": "NODE_ENV=production ts-node fuse",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "coverage": "yarn test --coverage --no-cache",
    "dist": "electron-builder --mac --win --linux --ia32 --x64",
    "draft": "electron-builder -p always --draft --mac --win --linux --ia32 --x64",
    "format": "prettier --write **/*.ts && tslint -p . --fix",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "precommit": "lint-staged",
    "prepush": "npm-run-all typecheck lint",
    "sanity": "npm-run-all clean typecheck lint test clean compile clean",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit --pretty"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "AWS",
    "Desktop",
    "Amazon Web Services"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.0.6",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.2",
    "electron-window-state-manager": "^0.3.2",
    "glamor": "^2.20.40",
    "mousetrap": "^1.6.5",
    "popmotion": "^8.7.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@types/electron-is-dev": "^1.1.1",
    "@types/electron-store": "^3.2.0",
    "@types/jest": "^25.1.3",
    "@types/mousetrap": "^1.6.3",
    "coveralls": "^3.0.9",
    "electron": "^18.3.7",
    "electron-builder": "^22.3.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-expect-contain-deep": "^1.0.1",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.2"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false,
    "mapCoverage": true
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
